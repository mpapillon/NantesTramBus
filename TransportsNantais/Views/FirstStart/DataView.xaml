                    </Ellipse.RenderTransform>
                </Ellipse>
                <Rectangle x:Name="rectangle1" Fill="{StaticResource PhoneForegroundBrush}" Height="7" Canvas.Left="70" Canvas.Top="155" Width="208" RenderTransformOrigin="0.5,0.5" Opacity="0">
                    <Rectangle.RenderTransform>
