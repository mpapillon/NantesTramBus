                </Rectangle>
                <TextBlock x:Name="textBlock" Canvas.Left="76" TextWrapping="Wrap" Text="envoyer un commentaire" Canvas.Top="270" RenderTransformOrigin="0.5,0.5" Opacity="0">
                    <TextBlock.RenderTransform>
