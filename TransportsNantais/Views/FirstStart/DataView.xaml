                    </TextBlock.RenderTransform>
                </TextBlock>
                <Ellipse x:Name="ellipse_Copy" Height="57" Canvas.Left="328" Canvas.Top="250" Width="57" RenderTransformOrigin="0.5,0.5" Fill="{StaticResource PhoneBorderBrush}" Opacity="0">
                    <Ellipse.RenderTransform>
