            <Grid.RenderTransform>
                <CompositeTransform/>
            </Grid.RenderTransform>
    		<Grid.ColumnDefinitions>
    			<ColumnDefinition/>
    			<ColumnDefinition/>
    		</Grid.ColumnDefinitions>
            <Button x:Name="BackBtn" Content="précédent" VerticalAlignment="Top" 
                    Click="BackBtn_Click" RenderTransformOrigin="0.5,0.5">
                <Button.RenderTransform>
                    <CompositeTransform/>
                </Button.RenderTransform>
            </Button>
            <Button x:Name="NextBtn" Content="terminer" Grid.Column="1" 
                    VerticalAlignment="Top" Click="NextBtn_Click" RenderTransformOrigin="0.5,0.5">
                <Button.RenderTransform>
                    <CompositeTransform/>
                </Button.RenderTransform>
            </Button>
    	</Grid>
        <Grid Margin="22,27,22,0">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition/>
            </Grid.RowDefinitions>
            <TextBlock HorizontalAlignment="Left" TextWrapping="Wrap" Text="Aidez-nous !" VerticalAlignment="Top" FontFamily="{StaticResource PhoneFontFamilyLight}" FontSize="{StaticResource PhoneFontSizeExtraLarge}" Margin="0,0,0,17"/>
            <TextBlock TextWrapping="Wrap" FontSize="{StaticResource PhoneFontSizeNormal}" Style="{StaticResource PhoneTextNormalStyle}" Margin="0" Grid.Row="1">
    			<Run FontWeight="SemiBold" Text="Nantes Tram &amp; Bus "/>
    			<Run Text="est encore jeune et a besoin de vous."/>
    			<LineBreak/>
    			<Run/>
    			<LineBreak/>
    			<Run Text="Si vous rencontrez un problème, que les données affichées ne sont pas correctes ou même si vous avez une idée. N'hésitez surtout pas à nous le faire partager !"/>
            </TextBlock>
            <Canvas Grid.RowSpan="2" Grid.Row="2">
                <Rectangle x:Name="rectangle" Fill="{StaticResource PhoneChromeBrush}" Height="51" Canvas.Left="46" Canvas.Top="264" Width="343" RenderTransformOrigin="0.5,0.5">
