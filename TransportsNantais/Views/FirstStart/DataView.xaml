                    </Ellipse.RenderTransform>
                </Ellipse>
                <Ellipse x:Name="ellipse" Height="57" Canvas.Left="333" Canvas.Top="250" Width="57" RenderTransformOrigin="0.5,0.5" Fill="{StaticResource PhoneBorderBrush}" Opacity="0">
                    <Ellipse.RenderTransform>
